{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["air-base"],
    "parse": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "lines-between-class-members": "off",
        "class-methods-use-this": "off"
    }
}
